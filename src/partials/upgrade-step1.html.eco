                <br>

 <input class="text-box single-line" data-val="true" data-val-required=" " id="Captcha" name="Captcha" type="text" value="">


                <span class="field-validation-valid" data-valmsg-for="Captcha" data-valmsg-replace="true"></span>
            </div>
        </div>
        <div class="form-row form-actions">
            <div class="wrapper-forward">
                <input type="submit" value="Continue »" class="btn_sbmit_imge">
            </div>
        </div>
    </div>



    <script type="text/javascript">
        $(document).ready(function () {
            //addErrorsToValidationSummary("error"); //test
        });
        function addErrorsToValidationSummary(errMsg) {

            var validationSummary = $('#validationSummary div.validation-summary-errors ul');

            if (validationSummary.length == 0) {
                $('#validationSummary').after('<ul></ul>');
                validationSummary = $('#validationSummary div.validation-summary-errors ul');
            }
            validationSummary.append('<li>' + errMsg + '</li>');

        }
    </script>



